{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "paths": {
      "Config": [
        "src/client/config/index.js"
      ],
      "Components/*": [
        "src/client/components/*"
      ],
      "Utils/*": [
        "src/client/utils/*"
      ],
      "@/*": [
        "src/client/*"
      ]
    }
  }
}
