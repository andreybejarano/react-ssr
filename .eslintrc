{
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "node": true
    },
    "settings": {
      "react": {
        "createClass": "createReactClass",
        "pragma": "React",
        "version": "detect",
        "flowVersion": "0.53"
      },
      "propWrapperFunctions": [
      ],
      "linkComponents": [
      ]
    },
    "plugins": [
      "react"
    ],
    "extends": [
      "standard",
      "plugin:react/recommended"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "globals": {
      "webapis": "writable",
      "tizen": "writable"
    },
    "rules": {
      "indent": [
        "error",
        2,
        {
          "SwitchCase": 1,
          "VariableDeclarator": 2
        }
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ],
      "space-before-function-paren": [
        "error",
        {
          "anonymous": "always",
          "named": "never"
        }
      ],
      "no-console": "off",
      "no-tabs": "off",
      "eqeqeq": "off",
      "react/prop-types": [
        0
      ],
      "react/no-find-dom-node": [
        0
      ]
    }
  }
